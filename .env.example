# TikTok ML System v4 - Production Environment Variables
# Copy this file to .env and fill in your actual credentials

# === SYSTEM CONFIGURATION ===
PRODUCTION_MODE=true
LOG_level=INFO
DEBUG=false

# === CORE API CONFIGURATION ===
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=1
API_SECRET_KEY=your_super_secret_api_key_here_min_32_chars
JWT_SECRET=your_jwt_secret_key_here_min_32_chars

# === SUPABASE CONFIGURATION ===
# Required for all metrics storage and database operations
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# === YOUTUBE API CONFIGURATION ===
# Required for YouTube analytics - Get from Google Cloud Console
YOUTUBE_API_KEY=your_youtube_data_api_key
YOUTUBE_CHANNEL_IDS=UCxxxxxx,UCyyyyyy,UCzzzzzz

# === META ADS CONFIGURATION ===
# Required for Meta advertising features - Get from https://developers.facebook.com
META_APP_ID=your_app_id_here
META_APP_SECRET=your_app_secret_here
META_ACCESS_TOKEN=your_long_lived_access_token
META_PIXEL_ID=your_meta_pixel_id

# === SPOTIFY API CONFIGURATION ===
# Required for Spotify analytics - Get from Spotify Developer Dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
SPOTIFY_ARTIST_IDS=4UXqAaa6dQYAk18Lv7PEgX,1vCWHaC5f2uS3yhpwWbIA6
SPOTIFY_PLAYLIST_IDS=37i9dQZF1DXcBWIGoYBM5M,37i9dQZF1DX0XUsuxWHRQd

# === LANDING PAGE CONFIGURATION ===
# Required for conversion tracking and performance monitoring
LANDING_PAGE_URLS=https://your-landing-page.com,https://your-second-page.com

# === ML CONFIGURATION ===
# Ultralytics YOLOv8 model configuration
ULTRALYTICS_MODEL=yolov8n.pt
ML_MODEL_PATH=/app/data/models
MAX_UPLOAD_SIZE=52428800
CONFIDENCE_THRESHOLD=0.5

# === n8n CONFIGURATION ===
# Required for workflow automation and orchestration
N8N_WEBHOOK_BASE_URL=https://your-n8n-instance.com/webhook
N8N_API_KEY=your_n8n_api_key

# === DATABASE CONFIGURATION ===
# Optional - uses Supabase by default
DATABASE_URL=postgresql://user:pass@host:port/dbname

# === DEPLOYMENT CONFIGURATION ===
# Railway deployment settings
RAILWAY_STATIC_URL=https://your-app.railway.app
PORT=8000

# Docker settings
DOCKER_ENV=production
HEALTHCHECK_INTERVAL=30s
