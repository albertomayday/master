# Device Farm v5 - Environment Configuration
# Copy to .env and fill with your actual values

# === GOLOGIN API ===
GOLOGIN_API_TOKEN=your_gologin_api_token_here

# === REDIS (Optional, for production task queue) ===
REDIS_PASSWORD=your_redis_password

# === DASHBOARD SECURITY ===
DASHBOARD_SECRET_KEY=your_dashboard_secret_key_here
DASHBOARD_USERNAME=admin
DASHBOARD_PASSWORD=your_dashboard_password

# === API SECURITY ===
API_SECRET_KEY=your_api_secret_key_here
JWT_SECRET_KEY=your_jwt_secret_key_here

# === ALERTS (Optional) ===
ALERT_WEBHOOK_URL=https://your-webhook-url.com/alerts
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# === ENCRYPTION (Optional) ===
ENCRYPTION_KEY=your_32_byte_encryption_key_here

# === DEVELOPMENT ===
ENVIRONMENT=production
DEBUG=false
LOG_LEVEL=INFO